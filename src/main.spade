// RV32I Core

#[no_mangle]
entity main(
    #[no_mangle] clk: clock,
    #[no_mangle] rst_n: bool,
) {

    // Stage 1: Instruction Fetch
    'pc // Program Counter
    'imem // Instruction Memory

    // Stage 2: Instruction Decode
    'decode // Decoder
    'rf // Register File

    // Stage 3: Execute
    'alu

    // Stage 4: Memory Access
    'dmem

    // Stage 5: Write Back to Register
    'wb

}
